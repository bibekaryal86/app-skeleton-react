{
  "name": "react-spa-skeleton",
  "version": "1.0.1",
  "description": "This is a template repository to create new React Application",
  "private": true,
  "main": "index.js",
  "scripts": {
    "start": "webpack serve --config webpack.dev.config.js",
    "build": "webpack --config webpack.prod.config.js",
    "prettier": "prettier --check \"**/*.+(js|jsx|ts|tsx)\"",
    "prettier:write": "prettier --write \"**/*.+(js|jsx|ts|tsx)\"",
    "lint": "eslint src/** --ext .js,.ts,.tsx",
    "lint:fix": "npm run lint -- --fix",
    "test": "jest --config ./jest.config.js --passWithNoTests",
    "test:u": "npm run test -- -u",
    "test:watch": "jest --config ./jest.config.js --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bibekaryal86/react-spa-skeleton.git"
  },
  "keywords": [
    "react",
    "redux",
    "typescript",
    "babel",
    "webpack",
    "eslint",
    "prettier",
    "jest"
  ],
  "author": "Bibek Aryal",
  "license": "ISC",
  "homepage": "https://github.com/bibekaryal86/react-spa-skeleton#readme",
  "dependencies": {
    "react": "^17.0.2",
    "react-csv": "^2.2.2",
    "react-dom": "^17.0.2",
    "react-redux": "^8.0.2",
    "react-router-dom": "^6.3.0",
    "redux": "^4.2.0",
    "redux-thunk": "^2.4.1",
    "styled-components": "^5.3.5"
  },
  "devDependencies": {
    "@babel/core": "^7.18.2",
    "@babel/plugin-transform-react-jsx": "^7.17.12",
    "@babel/plugin-transform-runtime": "^7.18.2",
    "@babel/preset-env": "^7.18.2",
    "@babel/preset-react": "^7.17.12",
    "@babel/preset-typescript": "^7.17.12",
    "@babel/runtime": "^7.18.3",
    "@types/enzyme": "^3.10.12",
    "@types/react": "^18.0.9",
    "@types/react-csv": "^1.1.2",
    "@types/react-dom": "^18.0.3",
    "@types/react-router-dom": "^5.3.3",
    "@types/redux-immutable-state-invariant": "^2.1.2",
    "@types/styled-components": "^5.1.25",
    "@typescript-eslint/eslint-plugin": "^5.26.0",
    "@typescript-eslint/parser": "^5.26.0",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.7",
    "babel-loader": "^8.2.5",
    "clean-webpack-plugin": "^4.0.0",
    "dotenv-webpack": "^7.1.0",
    "enzyme": "^3.11.0",
    "enzyme-to-json": "^3.6.2",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.30.0",
    "eslint-plugin-react-hooks": "^4.5.0",
    "eslint-webpack-plugin": "^3.1.1",
    "fork-ts-checker-webpack-plugin": "^7.2.11",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^27.5.1",
    "jest-cli": "^28.1.0",
    "jest-html-reporter": "^3.5.0",
    "jest-localstorage-mock": "^2.4.21",
    "jest-styled-components": "^7.0.8",
    "redux-devtools-extension": "^2.13.9",
    "redux-immutable-state-invariant": "^2.1.0",
    "redux-mock-store": "^1.5.4",
    "ts-jest": "^27.1.4",
    "typescript": "^4.7.2",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.9.1"
  }
}
